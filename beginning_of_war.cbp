<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Beginning Of War" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/beginning_of_war" prefix_auto="1" extension_auto="1" />
				<Option working_dir="$(#glut)/bin" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Winline" />
					<Add option="-Wunreachable-code" />
					<Add option="-Wzero-as-null-pointer-constant" />
					<Add option="-Wmain" />
					<Add option="-std=c++1z" />
					<Add option="-Og" />
					<Add option="-g" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-O2" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#glut.include)" />
		</Compiler>
		<Linker>
			<Add library="freeglut" />
			<Add library="opengl32" />
			<Add library="glu32" />
			<Add library="winmm" />
			<Add library="gdi32" />
			<Add directory="$(#glut.lib)" />
		</Linker>
		<Unit filename="common.h" />
		<Unit filename="constants.h" />
		<Unit filename="include/Game.h" />
		<Unit filename="include/GameObject.h" />
		<Unit filename="include/Player.h" />
		<Unit filename="main.cpp" />
		<Unit filename="src/Game.cpp" />
		<Unit filename="src/GameObject.cpp" />
		<Unit filename="src/Player.cpp" />
		<Unit filename="utils.cpp" />
		<Unit filename="utils.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
